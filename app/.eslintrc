{
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "React": true,
    "describe": false,
    "it": false,
    "before": false,
    "beforeEach": false,
    "after": false,
    "afterEach": false,
    "spyOn": false,
    "expect": false,
    "beforeAll": false
  },
  "rules": {
    "camelcase": 2,
    "curly": 2,
    "brace-style": [2, "1tbs"],
    "quotes": [2, "single"],
    "semi": [2, "always"],
    "object-curly-spacing": [2, "never"],
    "computed-property-spacing": [2, "never"],
    "array-bracket-spacing": 2,
    "eqeqeq": 2,
    "strict": [2, "global"],
    "indent": [2, 2],
    "no-magic-numbers": 2

  },
  "plugins": [
    "react"
  ],
  "parser": "babel-eslint"
}